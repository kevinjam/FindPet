{"generated":{"line":69,"column":3},"source":"findPetApp/js/main.js","original":{"line":58,"column":1}},{"generated":{"line":70,"column":0},"source":"findPetApp/js/main.js","original":{"line":92,"column":1}}],"sources":{"findPetApp/js/main.js":"import fetchJsonp from 'fetch-jsonp';\n\nimport { isValidZip, showAlert } from './validate';\nconst petForm = document.querySelector('#pet-form');\n\npetForm.addEventListener('submit', fetchAnimals);\n\n//Fetch Animals from API\nfunction fetchAnimals(e){\ne.preventDefault();\n\n\n//Get User Input\nconst animal = document.querySelector('#animal').value;\nconst zip = document.querySelector('#zip').value;\n\n// Validate Zip\n  if (!isValidZip(zip)) {\n    showAlert('Please Enter A Valid Zipcode', 'danger');\n    return;\n  }\n\n\n//Fetch Pets\n// fetchJsonp('http://api.petfinder.com/pet.find?format=json&key=&animal=${animal}&location=${zip}&callback=callback', {\n// \tjsonpCallbackFunction: 'callback'\n// })\n// \t.then(res=>res.json())\n// \t.then(data => console.log(data))\n// \t.catch(err=> console.log(err));\n\n// Fetch Pets\n  fetchJsonp(\n    `http://api.petfinder.com/pet.find?format=json&key=12c87483d09b9a507b3a365ee9eb87ea&animal=${animal}&location=${zip}&callback=callback`,\n    {\n      jsonpCallbackFunction: 'callback'\n    }\n  )\n    .then(res => res.json())\n    .then(data => showAnimals(data.petfinder.pets.pet))\n    .catch(err => console.log(err));\n}\n\n\n//SONP Callback\n\nfunction callback(data){\n\tconsole.log(data);\n}\n\n// show listing of pet\nfunction showAnimals(pets){\n\tconst results = document.querySelector('#results');\n\n// Clear First\n\tresults.innerHTML ='';\n\t//loop through\n\tpets.forEach(pet => {\n    console.log(pet);\n    const div = document.createElement('div');\n    div.classList.add('card', 'card-body', 'mb-3');\n    div.innerHTML = `\n      <div class=\"row\">\n        <div class=\"col-sm-6\">\n          <h4>${pet.name.$t} (${pet.age.$t})</h4>\n          <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\n          <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${\n      pet.contact.state.$t\n    } ${pet.contact.zip.$t}</p>\n          <ul class=\"list-group\">\n            <li class=\"list-group-item\">Phone: ${pet.contact.phone.$t}</li>\n            ${\n              pet.contact.email.$t\n                ? `<li class=\"list-group-item\">Email: ${\n                    pet.contact.email.$t\n                  }</li>`\n                : ``\n            }\n            <li class=\"list-group-item\">Shelter ID: ${pet.shelterId.$t}</li>\n          </ul>\n        </div>\n        <div class=\"col-sm-6 text-center\">\n          <img class=\"img-fluid rounded-circle mt-2\" src=\"${\n            pet.media.photos.photo[3].$t\n          }\">\n        </div>\n      </div>\n    `;\n\n    results.appendChild(div);\n  });\n}"},"lineCount":null}},"hash":"51734b478ff8bc4d2d60f6fdcb68288a","cacheData":{"env":{}},"id":8}