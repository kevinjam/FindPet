llback(data){\n\tconsole.log(data);\n}\n\n// show listing of pet\nfunction showAnimals(pets){\n\tconst results = document.querySelector('#results');\n\n// Clear First\n\tresults.innerHTML ='';\n\t//loop through\n\tpets.forEach(pet => {\n    console.log(pet);\n    const div = document.createElement('div');\n    div.classList.add('card', 'card-body', 'mb-3');\n    div.innerHTML = `\n      <div class=\"row\">\n        <div class=\"col-sm-6\">\n          <h4>${pet.name.$t} (${pet.age.$t})</h4>\n          <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\n          <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${\n      pet.contact.state.$t\n    } ${pet.contact.zip.$t}</p>\n          <ul class=\"list-group\">\n            <li class=\"list-group-item\">Phone: ${pet.contact.phone.$t}</li>\n            ${\n              pet.contact.email.$t\n                ? `<li class=\"list-group-item\">Email: ${\n                    pet.contact.email.$t\n                  }</li>`\n                : ``\n            }\n            <li class=\"list-group-item\">Shelter ID: ${pet.shelterId.$t}</li>\n          </ul>\n        </div>\n        <div class=\"col-sm-6 text-center\">\n          <img class=\"img-fluid rounded-circle mt-2\" src=\"${\n            pet.media.photos.photo[3].$t\n          }\">\n        </div>\n      </div>\n    `;\n\n    results.appendChild(div);\n  });\n}"},"lineCount":null}},"hash":"1866dea54b08a8595efe815e78ca9fc7","cacheData":{"env":{}},"id":6}